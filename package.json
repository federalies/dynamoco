{
  "name": "@federalies/dynamoco",
  "version": "0.5.0",
  "description": "A small but useful set of helpers, making Dynamo more acute and less obtuse. (Member of the Federalies)",
  "keywords": [
    "dynamo",
    "query",
    "scan",
    "helper",
    "query builder",
    "modands",
    "fluent",
    "single dependency",
    "federalies"
  ],
  "main": "build/src/index.js",
  "types": "src/index.ts",
  "files": [
    "build/src"
  ],
  "engines": {
    "node": ">=10.8.0"
  },
  "author": "Eric D Moore",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/federalies/dynamoco"
  },
  "scripts": {
    "dockerPull": "docker pull amazon/dynamodb-local",
    "dockerStart": "docker run -p 8000:8000 amazon/dynamodb-local &>/dev/null &",
    "reportCov": "cat .coverage/lcov.info | node -r dotenv/config node_modules/coveralls/bin/coveralls.js",
    "test": "nyc ts-node test/index.test.ts && nyc report --reporter=text-lcov > .coverage/lcov.info",
    "build": "rm -rf build && tsc && cp utils/reserved.txt.br build/src/reserved.txt.br",
    "publishPkg": "np",
    "validate": "ts-node utils/validate.ts",
    "publish:before": "npm run build && npm run test",
    "publish:dryrun": "np --preview --no-yarn",
    "lint": "eslint ./src/*.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.669.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate && npm run build && npm test",
      "pre-push": "npm test && npm run reportCov"
    }
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/deep-diff": "^1.0.0",
    "@types/is-ci": "^2.0.0",
    "@types/js-yaml": "^3.12.4",
    "@types/json5": "0.0.30",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chalk": "^4.0.0",
    "coveralls": "^3.1.0",
    "deep-diff": "^1.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25",
    "globby": "^11.0.0",
    "husky": "^4.2.5",
    "is-ci": "^2.0.0",
    "js-yaml": "^3.14.0",
    "json5": "^2.1.3",
    "node-fetch": "^2.6.0",
    "np": "^6.2.3",
    "nyc": "^15.0.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
