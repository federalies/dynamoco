    "query",
    "scan",
    "helper",
    "query builder",
    "modands",
    "fluent",
    "single dependency",
    "federalies"
  ],
  "main": "build/src/index.js",
  "types": "src/index.ts",
  "files": [
    "build/src"
  ],
  "scripts": {
